{
  "name": "xlib",
  "version": "0.0.1",
  "description": "tutorial proj",
  "main": "lib/_main.js",
  "module": "lib-esm/_main.js",
  "esnext": "lib-esm/_main.js",
  "browser": "lib-esm/_main.js",
  "typings": "lib/_main.d.ts",
  "scripts": {
    "build": "heft test --clean",
    "heft": "heft",
    "watch": "heft build --clean ;CI=true heft test --watch",
    "test": "heft test",
    "eslint-fix": "eslint ./src/**/*.ts* --fix",
    "dep-check-major": "CI=true npm-check-updates --packageFile package.json --removeRange --errorLevel 2",
    "dep-check": "CI=true npm-check-updates --packageFile package.json --removeRange --errorLevel 2 --target minor",
    "dep-upgrade-nc": "NPM_CHECK_INSTALLER=pnpm CI=true npm-check --skip-unused --update-all",
    "dep-upgrade": "npm-check-updates --packageFile package.json --upgrade --removeRange --target minor"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@rushstack/eslint-config": "~1.3.0",
    "@rushstack/heft": "~0.11.1",
    "@types/jest": "~26.0.13",
    "@types/node": "14.0.27",
    "@xlib/build-tools": "workspace:*",
    "@xlib/eslint-config": "workspace:*",
    "eslint": "^7.9.0"
  },
  "dependencies": {
    "gaxios": "^3.1.0",
    "ky": "0.23.0",
    "ky-universal": "0.8.1",
    "web-streams-polyfill": "3.0.0"
  }
}
