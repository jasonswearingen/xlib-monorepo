{
  "name": "@xlib/xlib-browser-webpack-basic",
  "version": "0.0.0",
  "description": "example using xlib in a browser app",
  "scripts": {
    "start-node": "node lib/_main.js",
    "start": "heft start",
    "build": "heft test --clean",
    "heft": "heft",
    "watch": "heft build --clean ; heft test --watch",
    "eslint-fix": "eslint ./src/**/*.ts* --fix",
    "dep-check": "NPM_CHECK_INSTALLER=pnpm CI=true npm-check --skip-unused",
    "dep-upgrade": "NPM_CHECK_INSTALLER=pnpm CI=true npm-check --skip-unused --update-all"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@rushstack/heft": "~0.11.1",
    "@types/jest": "~26.0.13",
    "@types/node": "~14.10.1",
    "@types/react": "~16.9.46",
    "@types/react-dom": "^16.9.8",
    "@types/webpack-env": "~1.15.2",
    "@xlib/build-tools": "workspace:*",
    "@xlib/eslint-config": "workspace:*",
    "css-loader": "4.3.0",
    "eslint": "~7.8.1",
    "html-webpack-plugin": "4.4.1",
    "style-loader": "~1.2.1",
    "typescript": "~4.0.2",
    "webpack": "~4.44.1"
  },
  "dependencies": {
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "xlib": "workspace:*"
  }
