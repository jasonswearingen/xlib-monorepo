{
  "name": "@xlib/ts-debug-test",
  "version": "0.0.1",
  "description": "verify typescript debug",
  "main": "lib/_main.js",
  "typings": "lib/_main.d.ts",
  "scripts": {
    "start": "node lib/_main.js",
    "build": "heft test --clean",
    "heft": "heft",
    "watch": "heft build --clean ; CI=true heft test --watch",
    "eslint-fix": "eslint ./src/**/*.ts* --fix",
    "dep-check-nc": "NPM_CHECK_INSTALLER=pnpm CI=true npm-check --skip-unused",
    "dep-check": "CI=true npm-check-updates --packageFile package.json --removeRange --errorLevel 2",
    "dep-upgrade-nc": "NPM_CHECK_INSTALLER=pnpm CI=true npm-check --skip-unused --update-all",
    "dep-upgrade": "npm-check-updates --packageFile package.json --upgrade --removeRange"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@rushstack/eslint-config": "~1.3.0",
    "@rushstack/heft": "~0.11.1",
    "@types/jest": "~26.0.13",
    "@types/node": "~14.10.1",
    "typescript": "~4.0.2",
    "@xlib/build-tools": "workspace:*",
    "@xlib/eslint-config": "workspace:*",
    "@typescript-eslint/eslint-plugin": "~4.1.0",
    "@typescript-eslint/parser": "~4.1.0",
    "eslint": "~7.8.1",
    "eslint-plugin-jest": "~24.0.0",
    "eslint-plugin-react": "~7.20.6"
  },
  "dependencies": {
    "xlib": "workspace:*"
  }
}